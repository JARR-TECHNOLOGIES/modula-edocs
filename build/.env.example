# Container core
TZ=America/Costa_Rica

# Logging
LOG_LEVEL=DEBUG

# MongoDB
MONGO_USERNAME=change-me-username
MONGO_PASSWORD=change-me-password
MONGO_CLUSTER=change-me-cluster.mongodb.net

# Gunicorn
GUNICORN_WORKERS=1
GUNICORN_THREADS=6
GUNICORN_TIMEOUT=120
GUNICORN_KEEPALIVE=5
GUNICORN_MAX_REQUESTS=1000
GUNICORN_MAX_REQUESTS_JITTER=200
GUNICORN_EXTRA_ARGS=--reload
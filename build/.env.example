# Container core
TZ=America/Costa_Rica

# Logging
LOG_LEVEL=DEBUG

# API
FILES_API_SECRET=change-me
FILES_API_KEY=change-me
FILES_ROOT=/gcp-bucket

# Gunicorn
GUNICORN_WORKERS=1
GUNICORN_THREADS=6
GUNICORN_TIMEOUT=120
GUNICORN_KEEPALIVE=5
GUNICORN_MAX_REQUESTS=1000
GUNICORN_MAX_REQUESTS_JITTER=200
GUNICORN_EXTRA_ARGS=--preload